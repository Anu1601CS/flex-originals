{
  "name": "audio",
  "plural": "audios",
  "base": "PersistedModel",
  "idInjection": true,
  "options": {
    "validateUpsert": true
  },
  "hidden": [
    "audioOwnerId"
  ],
  "properties": {
    "title": {
      "type": "string",
      "required": true,
      "default": "Untitled"
    },
    "audioMeta": {
      "type": "object",
      "required": true,
      "default": {}
    },
    "audioFile": {
      "type": "string",
      "required": true
    },
    "visibility": {
      "type": "number",
      "required": true,
      "default": 0
    },
    "description": {
      "type": "string"
    },
    "thumbImage": {
      "type": "string"
    },
    "gif": {
      "type": "string"
    },
    "thumbnails": {
      "type": "object"
    },
    "tags": {
      "type": "string"
    },
    "playlist": {
      "type": "object"
    },
    "location": {
      "type": "string"
    },
    "category": {
      "type": "string"
    },
    "allowComments": {
      "type": "boolean",
      "required": true,
      "default": true
    },
    "agerestriction": {
      "type": "boolean",
      "required": true,
      "default": false
    },
    "ratings": {
      "type": "number"
    },
    "licence": {
      "type": "string",
      "required": true,
      "default": "Flex Originals"
    },
    "likedPrivate": {
      "type": "boolean",
      "required": true,
      "default": false
    }
  },
  "validations": [],
  "relations": {
    "user": {
      "type": "belongsTo",
      "model": "user",
      "foreignKey": "audioOwnerId"
    },
    "audioanalytics": {
      "type": "hasOne",
      "model": "Audioanalytic",
      "foreignKey": "audioId"
    }
  },
  "acls": [
    {
      "accessType": "*",
      "principalType": "ROLE",
      "principalId": "$everyone",
      "permission": "DENY"
    },
    {
      "accessType": "EXECUTE",
      "principalType": "ROLE",
      "principalId": "$owner",
      "permission": "ALLOW",
      "property": "publish"
    },
    {
      "accessType": "EXECUTE",
      "principalType": "ROLE",
      "principalId": "$owner",
      "permission": "ALLOW",
      "property": "deleteItem"
    },
    {
      "accessType": "EXECUTE",
      "principalType": "ROLE",
      "principalId": "$owner",
      "permission": "ALLOW",
      "property": "getAudioInfo"
    }
  ],
  "methods": {}
}
