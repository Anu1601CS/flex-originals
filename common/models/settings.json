{
  "name": "settings",
  "plural": "settings",
  "base": "PersistedModel",
  "idInjection": true,
  "options": {
    "validateUpsert": true
  },
  "properties": {
    "emailNotification": {
      "type": "boolean",
      "required": true,
      "default": true
    },
    "desktopNotification": {
      "type": "boolean",
      "required": true,
      "default": false
    },
    "verifiedChannel": {
      "type": "boolean",
      "required": true,
      "default": false
    },
    "followers": {
      "type": "number",
      "required": true,
      "default": 0
    },
    "theme": {
      "type": "string",
      "required": true,
      "default": "dark"
    },
    "facebook": {
      "type": "string"
    },
    "instagram": {
      "type": "string"
    },
    "twitter": {
      "type": "string"
    },
    "redit": {
      "type": "string"
    },
    "linkedin": {
      "type": "string"
    }
  },
  "validations": [],
  "relations": {
    "user": {
      "type": "belongsTo",
      "model": "user",
      "foreignKey": "ownerId"
    }
  },
  "acls": [
    {
      "accessType": "*",
      "principalType": "ROLE",
      "principalId": "$everyone",
      "permission": "DENY"
    },
    {
      "accessType": "EXECUTE",
      "principalType": "ROLE",
      "principalId": "$owner",
      "permission": "ALLOW",
      "property": "findSetting"
    }
  ],
  "methods": {}
}
